<script>
  const subMenu = document.querySelector(
    ".dropdown-submenu.menu-dropdown-shadow li.group-flyout"
  );
  const menuItems = document.querySelectorAll(
    ".desktop-menu li.group:not(.n-pricing-menu):not(.n-contact-us-menu):not(.n-get-started-free-menu)"
  );
  const menuItemsTop = document.querySelectorAll(
    ".desktop-top-menu li.group:not(:nth-child(3)):not(:nth-child(4))"
  );
  const searchButton = document.querySelector("li#SearchButton");

  function addHiddenClass(list) {
    list.forEach((menuItem) =>
      document.querySelector(`#${menuItem.id} div`).classList.add("hidden")
    );
  }

  function addMouseEvents(menuItem, menuItems) {
    addMenuKeypressEvents(menuItem, menuItems);
    menuItem.addEventListener("mouseleave", (event) => {
      addHiddenClass(menuItems);
      subMenu.classList.remove("mouse-show");
    });
    menuItem.addEventListener("mouseenter", (event) => {
      addHiddenClass(menuItems);
      subMenu.classList.remove("mouse-show");
    });
  }

  function addMenuKeypressEvents(menuItem, menuItems) {
    menuItem.addEventListener("keypress", (event) => {
      const menuContent = document.querySelector(`#${menuItem.id} div`);

      if (
        event.key === "Enter" &&
        !event.target.classList.contains("group-flyout")
      ) {
        event.preventDefault();

        if (!menuContent.classList.contains("hidden")) {
          menuContent.classList.add("hidden");
        } else if (menuContent.classList.contains("hidden")) {
          addHiddenClass(menuItems);
          menuContent.classList.remove("hidden");
        }
      }
    });
  }

  subMenu.addEventListener("keypress", (event) => {
    if (event.key === "Enter") {
      event.preventDefault();
      subMenu.classList.toggle("mouse-show");
    }
  });

  window.addEventListener("click", (e) => {
    addHiddenClass(menuItems);
    addHiddenClass(menuItemsTop);
    subMenu.classList.remove("mouse-show");
  });

  // FOR MAIN MENU
  menuItems.forEach((menuItem) => {
    addMouseEvents(menuItem, menuItems);
  });

  // FOR TOP MENU
  menuItemsTop.forEach((menuItem) => {
    addMouseEvents(menuItem, menuItemsTop);
  });

  searchButton.addEventListener("keypress", (event) => {
    searchButton.click();
  });
</script>

