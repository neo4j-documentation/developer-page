      <script>
        document
          .querySelectorAll(
            '[data-toggle="toggle-neo4j-mobile-menu-container"]'
          )
          .forEach((toggle) => {
            const menu = document.getElementById(
              "toggle-neo4j-mobile-menu-container"
            );

            if (toggle && menu) {
              const [container] = menu.getElementsByClassName(
                "js-off-canvas-overlay"
              );
              toggle.addEventListener("click", () => {
                const hidden = menu.getAttribute("aria-hidden");
                if (hidden === "true") {
                  menu.setAttribute("aria-hidden", "false");
                  menu.classList.add("is-open");
                  menu.classList.remove("is-closed");

                  if (container) {
                    container.classList.add("is-visible");
                    container.classList.add("is-closable");
                    container.classList.remove("is-closed");
                  }
                } else {
                  menu.setAttribute("aria-hidden", "true");
                  menu.classList.remove("is-open");
                  menu.classList.add("is-closed");

                  if (container) {
                    container.classList.remove("is-visible");
                    container.classList.remove("is-closable");
                    container.classList.add("is-closed");
                  }
                }
              });
            }
          });

        document
          .querySelectorAll(".is-accordion-submenu-parent > a[role=menuitem]")
          .forEach((anchor) => {
            anchor.addEventListener("click", () => {
              const parent = anchor.parentElement;
              if (!parent) return;

              const submenu = parent.querySelector(".is-accordion-submenu");
              if (!submenu) return;

              const expanded = parent.getAttribute("aria-expanded");

              if (expanded === "true") {
                parent.setAttribute("aria-expanded", "false");
                submenu.classList.remove("is-active");
                submenu.setAttribute("aria-hidden", "true");
                submenu.style.display = "none";
              } else {
                parent.setAttribute("aria-expanded", "true");
                submenu.classList.add("is-active");
                submenu.setAttribute("aria-hidden", "false");
                submenu.style.display = "block";
              }
            });
          });
      </script>
